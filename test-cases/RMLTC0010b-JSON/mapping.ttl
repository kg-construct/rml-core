@prefix ex: <http://example.com/> .
@prefix ql: <http://w3id.org/rml/ql/> .
@prefix rml: <http://w3id.org/rml/> .

<http://example.com/base/TriplesMap1> a rml:TriplesMap ;
    rml:logicalSource [ a rml:LogicalSource ;
            rml:iterator "$.countries[*]" ;
            rml:referenceFormulation ql:JSONPath ;
            rml:source <_:b461739> ] ;
    rml:predicateObjectMap [ rml:objectMap [ rml:reference "Name" ] ;
            rml:predicate ex:name ] ;
    rml:subjectMap [ rml:template "http://example.com/{$.['Country Code']}/{Name}" ] .

<_:b461739> a rml:RelativePathSource ;
    rml:path "country_info.json" .

