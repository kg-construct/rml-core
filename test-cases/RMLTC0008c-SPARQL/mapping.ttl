@prefix ex: <http://example.com/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rml: <http://w3id.org/rml/> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .

<http://example.com/base/TriplesMap1> a rml:TriplesMap ;
    rml:logicalSource [ a rml:LogicalSource ;
            rml:iterator "$.results.bindings[*]" ;
            rml:query """
              PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
              PREFIX foaf: <http://xmlns.com/foaf/0.1/>
              PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
              SELECT ?Name ?ID
              WHERE {
                  ?x  foaf:name ?Name ;
                      rdf:ID    ?ID ;
              } """ ;
            rml:referenceFormulation rml:JSONPath ;
            rml:source _:b836520 ] ;
    rml:predicateObjectMap [ rml:objectMap [ rml:reference "Name.value" ] ;
            rml:predicate ex:name,
                foaf:name ] ;
    rml:subjectMap [ rml:template "http://example.com/Student/{ID.value}/{Name.value}" ] .

_:b836520 a rml:RelativePathSource ;
    rml:path <http://example.com/base/#InputSPARQL> .

<http://example.com/base/#InputSPARQL> a sd:Service ;
    sd:endpoint <http://localhost:PORT/ds1/sparql> ;
    sd:resultFormat <http://www.w3.org/ns/formats/SPARQL_Results_JSON> ;
    sd:supportedLanguage sd:SPARQL11Query .

